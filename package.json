{
  "name": "@genestack/ui",
  "version": "12.12.3",
  "description": "UI library",
  "main": "dist/",
  "types": "dist/",
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/genestack/apparatuis"
  },
  "publishConfig": {
    "registry": "https://nexus.devops.gs.team/repository/npm-releases"
  },
  "keywords": [
    "genestack",
    "ui"
  ],
