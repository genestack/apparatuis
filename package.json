    "@testing-library/react": "^12.1.5",
    "@types/autoprefixer": "^9.7.2",
    "@types/extract-text-webpack-plugin": "^3.0.7",
    "@types/jest": "^29.0.3",
    "@types/node": "^10.14.1",
    "@types/react": "^17.0.59",
    "@types/react-dom": "^17.0.20",
    "@types/react-transition-group": "^2.9.2",
    "autoprefixer": "^10.4.12",
    "copy-webpack-plugin": "^11.0.0",
