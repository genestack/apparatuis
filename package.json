{
  "name": "genestack-ui",
  "version": "3.0.1",
  "description": "UI library",
  "main": "dist/index.js",
  "types": "src/",
  "scripts": {
    "build": "rm -rf dist && mkdir dist && tsc -p ./ && cpx  'src/**/*.css' './dist/'",
    "watch": "rm -rf dist && mkdir dist && tsc --watch -p ./ & cpx 'src/**/*.css' './dist/' -w",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/genestack/frontend-libraries/tree/master/ui-widgets/genestack-ui"
  },
  "keywords": [
    "genestack",
    "ui"
  ],
  "author": "Genestack Limited",
  "license": "UNLICENSED",
  "eslintConfig": {
    "extends": [
      "genestack-react"
    ],
    "settings": {
      "import/resolver": "webpack"
    },
    "rules": {
      "import/extensions": "off"
    }
  },
  "stylelint": {
    "extends": [
      "genestack-stylelint-config"
    ]
  },
  "babel": {
    "plugins": [
      "transform-runtime",
      "transform-object-rest-spread"
    ],
    "presets": [
      "env",
      "react"
    ]
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.3.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.9",
    "eslint": "^3.9.1",
    "eslint-config-genestack-react": "^0.2.1",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.5.1",
    "genestack-stylelint-config": "^1.0.1",
    "postcss-custom-properties": "^7.0.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.3",
    "react-styleguidist": "^6.2.5",
    "style-loader": "^0.20.2",
    "stylelint": "^9.1.1",
    "typescript": "^2.8.1",
    "webpack": "^3.11.0"
  },
  "dependencies": {
    "@types/rc-tooltip": "^3.4.11",
    "classnames": "^2.2.5",
    "extract-text-webpack-plugin": "^3.0.2",
    "prop-types": "^15.6.0",
    "rc-tooltip": "^3.7.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "peerDependencies": {
    "genestack-frontend-builder": "^0.5.0"
  }
}
