{
  "name": "@genestack/ui",
  "version": "13.0.3",
  "description": "UI library",
  "main": "dist/",
  "types": "dist/",
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/genestack/apparatuis"
  },
