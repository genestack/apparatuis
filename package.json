  "license": "UNLICENSED",
  "scripts": {
    "start": "styleguidist server",
    "build": "styleguidist build",
    "build:bundle": "npm run clean && webpack --env production",
    "prettier": "git ls-files | grep -E '\\.(js|jsx|ts|tsx|md|css|json)$' | xargs prettier",
    "prettier:fix": "prettier --write \"**/*.*\"",
    "prettier:check": "npm run prettier -- -l",
    "clean": "rm -rf dist",
    "tslint": "tslint -p .",
    "tscheck": "tsc --noEmit -p .",
    "jest": "jest -c ./jest.config.js",
    "test": "npm run tscheck && npm run jest"
  },
  "devDependencies": {
    "@genestack/tsconfig": "^2.0.0",
