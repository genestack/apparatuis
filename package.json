    "react": "^17.0.2",
    "react-docgen": "^5.4.3",
    "react-docgen-typescript": "^2.2.2",
    "react-dom": "^17.0.2",
    "react-styleguidist": "^12.0.1",
    "style-loader": "^3.3.1",
